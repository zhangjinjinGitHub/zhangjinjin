        $('.subnav-box [data-ring]').each(function () {
            var a = $(this).find('a');
            var ahref = a.attr('href');

            if (ahref == location.pathname) {
                $(this).addClass('eventn');
            }
        });

        $(".nav_starr li").hover(function () {
            $(this).find('span').show();
        }, function () {
            $(this).find('span').hide();
        });

        /* 今日金价 */
