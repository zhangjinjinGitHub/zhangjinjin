    function saveSuggest() {

        var liuyan = $(".liuyan").val(), suPhone = $(".su-phone").val();
        if (liuyan == "") {
            alert("请您输入您的建议和意见！")
        }
        if (suPhone == '') {
            alert('请输入您的电话号码！');
            return false;
        } else if (suPhone !== '') {
            var p1 = /(1[3-9]\d{9}$)/;
            var b = false;
            if (p1.test(suPhone) == b) {
                alert("请输入正确的号码格式！")
                $(this).focus();
                return false;
            }
        }
        $.get('/pc.php?m=Api&c=Subject&a=saveCustomFeedBack&suggest=' + liuyan + '&mobile=' + suPhone, function (res) {
            if (res.result === 1) {
                $(".su-phone").val("");
                $(".liuyan").val("");
                $(".jy-win .jy-img").hide();
                $(".jy-succ").show();
            } else {
                alert(res.msg);
            }
        }, 'json')

    }
    /*官网公用中间弹框*/
    var test = $(".head-top .whch");
    function kefu_show() {
        //过滤不需要弹窗的页面 (购物车和订单确认页，订单支付页，个人中心所有页面，登录注册页面，裸钻定制页,招商页面，品牌馆页面，预约到店页面，珠宝课堂页面，帮助中心页面，底部公用模块中的页面)
        var showStatus = true;
        var forbidPages = [
            '/home/diamond', '/home/diamond/index', '/Public/subject/201508/franch/', '/home/shop/home', '/zhubaoketang', '/home/index/frlnk/pos/homefooter', '/home/article/help',
            '/zhuanti', '/arc-top-help', '/Home/Map/index'
        ];
        /*用正则判断珠宝课堂文章里面的所有文章页面、个人中心及支付页面类、品牌类、底部公用模块中的页面，限制他们的弹窗*/
        if (jQuery.inArray(loc, forbidPages) != -1 || /^\/jew/.test(loc) || /^\/tag/.test(loc) || /^\/user/.test(loc) || /^\/home\/brand/.test(loc) || /^\/home\/article\/h_/.test(loc)) {
            showStatus = false;
        }
        if (showStatus) {
            $(".kefu_pop").show();
        }
    }
    $(".k_close").click(function () {
        $(".kefu_pop").hide();
    });
    window.setTimeout("kefu_show()", 15000);
    /*检测顶部该出现的页面*/
    function Top_show(){
        var toShow=false;
        if(/^\/home\/wedding\/home/.test(loc)){
            toShow=true;
        }
        if (toShow) {
            $(".national_day").show();
        }
    }
    Top_show();
</script>
