    window.onload = function () {
        //nanli左侧弹出框
        var l_test = $(".high_light_shop").text();
        var l_shopId = $(".high_light_shop").attr("data-shop-id");
        $(".bes_shop1").val(l_test);
        $(".bes_shop").val(l_shopId);

        $(".massa").show().animate({"left": -210}, "slow");
        ;
        var timer = setTimeout(function () {
            $(".massa").animate({"left": 0}, "slow").hide("slow");
        }, 5000);
        $(".massa .mi-a").click(function () {
            clearTimeout(timer);
            $(".massa").animate({"left": 0}, "slow").hide("slow");
        });
    }
