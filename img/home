<script type="text/javascript">
    $(function () {
        App.init();
        App.initStores();
        get_bespoke_count();
        KelaBespokeFunc.sureph();
        KelaBespokeFunc.setDatatime('.foot-third');
        KelaBespokeFunc.setShopOptions('.foot-third .bes_shop');
        KelaBespokeFunc.setDatatime('.foot-tow');
        KelaBespokeFunc.setShopOptions('.foot-tow .bes_shop');
        var shopname = $(".whereyu").find(".whch").html();

        if (shopname == "全国" || !($(".whereyu i").hasClass("whch"))) {
            $(".first-footer").css({"display": "none"});
            $(".second-footer").css({"display": "block"});
        } else {
            $(".first-footer").css({"display": "block"});
            $(".second-footer").css({"display": "none"});
        }
        $(".second-footer .bes_shop").change(function () {
            $(".more").css({"display": "none"});
            $(".foot-side").css({"display": "block"});
            //$(".foot-side h1").html($(".foot-tow .bes_shop").find("option:selected").html());

            //获取所选体验店id
            var selectedShop = parseInt($('.foot-tow .select-shop option:checked').val());
            $('.foot-tow').find('input[name="bes_shop"]').val(selectedShop);
            //根据所选体验店id请求接口，获取体验店信息
            $.get('/pc.php?m=Api&c=GetShop&a=getShopInfo&shop_id=' + selectedShop, function (res) {
                if (res.result === 1) {
                    console.log(res);
                    $('.foot-three .foot-side h1').text(res.data.shop_name);
                    $('.foot-three .shop_tel').text(res.data.shop_phone);
                    $('.foot-three .shop_time').text(res.data.shop_time);
                    $('.foot-three .shop_address').text(res.data.shop_address);

                }
            })

        })
    });

</script>

<script type="text/javascript">
    function get_bespoke_count() {
        if ($('.booking-box').length == 0) return;
        $.ajax({
            url: "/api/bespoke/getbespokecount",
            type: 'GET',
            dataType: "JSON",
            processData: "false",
            error: function () {
                $('.booking-box > h4').remove();
                $('.booking-box').append('<h4>累计预约人数 <b>统计中</b> 人</h4>')
            },
            success: function (data) {
                $('.booking-box > h4').remove();
                $('.booking-box').append('<h4>累计预约人数 <b>' + data + '</b> 人</h4>')
            }
        });
    }
</script>
