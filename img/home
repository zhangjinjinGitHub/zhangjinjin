    function isMobile(strMobile) {
        var patternMobile = /^1\d{10}$/;
        if (patternMobile.test(strMobile)) {
            return true;
        } else {
            return false;
        }
    }
    $(".phone").blur(function () {
        if (this.value == '' || this.value == '请输入您的手机号码') {
            return false;
        } else if (!isMobile(this.value)) {
            alert("手机号码格式不正确！");
            return false;
        }
    });
