<!---nanli  左侧弹窗框--->
<script type="text/javascript">
    //nanli  左侧弹窗框

    function showbar() {
        var i = $.cookie('sibar_show');
        if (parseInt(i)) {

            return true;
        } else {
            if (i !== '0') {
                return true;
            }
            return false;
        }
    }
    $(function () {
        //nanli 左侧弹出框
        $(".l_mark4").click(function () {
            $(".l_mark2").fadeIn();
        });
        $(".l_clolse").click(function () {
            $(".l_mark2").fadeOut();
        });
        $(".l_hide").click(function () {
            $.cookie('sibar_show', 0, {domain: '.kela.cn', path: '/'});
            $(".l_mark1").fadeOut();
        })
        $(".l_mark").click(function () {
            $.cookie('sibar_show', 1, {domain: '.kela.cn', path: '/'});
            $(".l_mark1").fadeIn();
        })
        // 判断显示还是隐藏
        thisURL = document.URL;
        tmpUPage = thisURL.split("/");
        thisUPage = tmpUPage[tmpUPage.length - 1];
        var l_shopId = $(".whch").text();
        var is_show = showbar();
        if (is_show) {
            if (l_shopId == '全国') {
                $(".l_mark").hide();
                $(".l_mark1").hide();

            } else {
                if (thisUPage == 'brand' || thisUPage == 'home' || thisUPage == 'franch' || thisUPage == 'qixi') {
                    $(".l_mark1").fadeOut();
                } else {
                    $(".l_mark1").fadeIn();
                }

            }


        } else {
            if (l_shopId == '全国') {
                $(".l_mark").hide();
                $(".l_mark1").hide();

            }

            $(".l_mark1").fadeOut();
        }
    })

</script>
