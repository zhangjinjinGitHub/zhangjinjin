<script>
    $(function () {
        $(window).scroll(function () {
            if ($(window).scrollTop() > 123) {
                $(".bottom_fiexd_box").fadeIn();
                $(".bottom_show_left").fadeIn();
            } else {
                $(".bottom_fiexd_box").fadeOut();
                $(".bottom_show_left").fadeOut();
            }
        });

        $(".hide_copu").on('click', function () {
            $(".bottom_fiexd_box").animate({left: "-100%"}, 500, function () {
                $(".bottom_show_left").animate({left: "0"});
            })
        });
        $(".bottom_show_left").on('click', function () {
            $(".bottom_show_left").animate({left: "-257px"}, 500, function () {
                $(".bottom_fiexd_box").animate({left: "0"});
            })
        });

        $(".coup_dialog .close,.btn_ture").click(function () {
            $(".mask").hide();
            $(".coup_dialog").hide();
        });

        //客资收集 外层容器 .user_research
        $(document).delegate('.user_research .submit', 'click', function () {
            $s = $(this).closest('.user_research');
            //手机号
            var phone = $s.find("input[name='phone']").val();
            var data = {};
            if (phone == '') {
                alert('手机号不能为空');
                return false;
            } else if (!isMobile(phone)) {
                alert('手机号码格式不正确');
                return false;
            }
            data['phone'] = phone;
            //姓名
            if ($s.find("input[name='name']").length) {
                var name = $s.find("input[name='name']").val();
                if (name == '') {
                    alert('请填写姓名');
                    return false;
                }
                data['name'] = name;
            }
            //预算
            if ($s.find("select[name='budget']").length) {
                var budget = $s.find("select[name='budget']").val();
                if (budget == '0') {
                    alert('请选择预算');
                    return false;
                }
                data['budget'] = budget;
            }
            //材质
            if ($s.find("select[name='caizhi']").length) {
                var caizhi = $s.find("select[name='caizhi']").val();
                if (caizhi == '0') {
                    alert('请选择材质');
                    return false;
                }
                data['caizhi'] = caizhi;
            }
            //款式
            if ($s.find("select[name='style']").length) {
                var style = $s.find("select[name='style']").val();
                if (style == '0') {
                    alert('请选择款式');
                    return false;
                }
                data['style'] = style;
            }
            $.post('/pc.php?m=home&c=wedding&a=user_research', data, function (res) {

                $(".mask").show();
                $(".coup_dialog").show();

            })
        })

        /* 隐藏左侧导航 */
        if ($(".l_mark").length) {
            $(".l_mark , .l_mark1").remove();
        }
    })

</script>
