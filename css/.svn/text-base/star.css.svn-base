div.mu{ width: 44px; height: 44px; margin: 0px 0px -44px 40px;
		animation: myrotate 1s linear infinite;
	}
@keyframes myrotate{
	from{
		transform: rotate(0deg);
	}
	to{
		transform: rotate(360deg);
	}
}
