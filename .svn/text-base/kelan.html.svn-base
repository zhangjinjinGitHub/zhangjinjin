	.as li:nth-child(5) a{display:none;width:94px;height:94px;border-radius:50%;background:url('img/02-3.png') no-repeat -563px 0px;}
	.as li:nth-child(6) a{display:none;width:94px;height:94px;border-radius:50%;background:url('img/02-3.png') no-repeat -700px 0px;}
	.as li:nth-child(7) a{display:none;width:94px;height:94px;border-radius:50%;background:url('img/02-3.png') no-repeat -840px 0px;}
	.as li:nth-child(8) a{display:none;width:96px;height:96px;border-radius:50%;background:url('img/02-3.png') no-repeat -980px 1px;}
	.as li:nth-child(9) a{display:none;width:96px;height:96px;border-radius:50%;background:url('img/02-3.png') no-repeat -1120px 0px;}
	.as li:nth-child(1){width:90px;height:90px;border-radius:50%;margin-right:50px;}
	.as li:nth-child(2){width:90px;height:90px;border-radius:50%;margin-right:50px;}
	.as li:nth-child(3){width:94px;height:94px;border-radius:50%;margin-right:45px;}
	.as li:nth-child(4){width:94px;height:94px;border-radius:50%;margin-right:50px;}
	.as li:nth-child(5){width:94px;height:94px;border-radius:50%;margin-right:45px;}
	.as li:nth-child(6){width:94px;height:94px;border-radius:50%;margin-right:45px;}
	.as li:nth-child(7){width:94px;height:94px;border-radius:50%;margin-right:47px;}
	.as li:nth-child(8){width:96px;height:96px;border-radius:50%;margin-right:41px;}
	.as li:nth-child(9){width:96px;height:96px;border-radius:50%;margin-right:0px;}
	body div:nth-child(2){margin:0 auto;width:366px;height:79px;margin-top:55px;margin-bottom:50px;}
	body div:nth-child(3){width:100%;height:102px;background:#fafafa;}
	body div:nth-child(3) ul{margin:0 auto;width:1105px;height:102px;}
	body div:nth-child(3) ul li{float:left;width:276px;height:102px;}
	body div:nth-child(3) ul li:nth-child(2) a{display:inline-block;width:275px;height:102px;background:url('img/03-2.jpg') no-repeat -276px 0px;}
	body div:nth-child(3) ul li:nth-child(3) a{display:inline-block;width:275px;height:102px;background:url('img/03-2.jpg') no-repeat -550px 0px;}
	body div:nth-child(3) ul li:nth-child(4) a{display:inline-block;width:275px;height:102px;background:url('img/03-2.jpg') no-repeat -826px 0px;}
	body div:nth-child(4){width:100%;height:451px;background:url('img/03bj_1.jpg') no-repeat -236px -100px;position:relative;}
	body div:nth-child(4) div:nth-child(1){position:absolute;left:965px;top:49px;}
	.first_a{display:inline-block;width:276px;height:102px;background:url('img/03-3.jpg') no-repeat;}
	.first_a-none{display:inline-block;width:276px;height:102px;background:url('img/03-2.jpg') no-repeat;}
	.center{background:#fafafa;}
	.center .center_1 a{float:right;margin-right:25px;padding-top:67px;color:#C69E49;border-bottom:1px solid #C69E49;font-size:14px;padding-bottom:5px;}
	.center .center_1 h3{float:left;padding-top:67px;font-weight:500;}
	.center .center_2 div{float:left;width:200px;height:560px;}
	.center .center_2 div img{display:block;margin-bottom:9px;}
	.center .center_2{height:596px;width:1038px;margin-top:13px;}
	.center_2 ul li{float:left;margin-left:9px;margin-bottom:10px;position:relative;}
	.center_2 ul li:hover{box-shadow:0px 6px 6px #999;top:-2px;}
	.center_3 dl dt{float:left;margin-right:17px;position:relative;}
	.center_3 dl dt:last-child{float:right;margin-right:0px;}
	.center_3 dl dt:hover{box-shadow:0px 5px 8px #999;top:-2px;}
</style>
<script>
	$(function (){
		(function (){
			//图片hover事件-切换背景
			$('.as li').hover(function (){
				$(this).children('a').css({display:'block'});
			},function (){
				$(this).children('a').css({display:'none'});
			});
			//图片click事件-切换图片
			$('.as_1 li').click(function (){
				var index=$(this).index();
				if(index!=0){
					if(index==1){
						$('body').children('div').eq(3).css({backgroundImage:"url('img/03bj_4.jpg')"});
						$('body').children('div').eq(3).children('div').css({height:'',top:'',left:'1000px'}).children('img').get(0).src="img/03-p2.png";
					};
					if(index==2){
						$('body').children('div').eq(3).css({backgroundImage:"url('img/03bj_2.jpg')"});
						$('body').children('div').eq(3).children('div').css({height:'',top:'',left:''}).children('img').get(0).src="img/jj-3.png";
					};
					if(index==3){
						$('body').children('div').eq(3).css({background:"url('img/03bj_3.jpg') no-repeat -236px -100px"});
						$('body').children('div').eq(3).children('div').css({height:'412px',top:'20px',left:'1010px'}).children('img').get(0).src="img/03-q4.png";
					};
					
					$('.as_1').children('li').eq(0).children('a').removeClass('first_a').addClass('first_a-none');
					$(this).children('a').css({backgroundImage:"url('img/03-3.jpg')"}).end().siblings().children('a').css({backgroundImage:''});
				}else{
					$('body').children('div').eq(3).css({backgroundImage:"url('img/03bj_1.jpg')"});
					$('.as_1').children('li').eq(0).children('a').removeClass('first_a-none').addClass('first_a');
					$('body').children('div').eq(3).children('div').css({height:'',top:''}).children('img').get(0).src="img/03-p2.png";
					$(this).siblings().children('a').css({backgroundImage:''});
				}
			});
		})();
	});
</script>
